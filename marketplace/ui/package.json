{
  "name": "marketplace-ui",
  "version": "0.1.0",
  "private": true,
  "description": "ILLUVRSE Marketplace UI (Next.js + TypeScript + Tailwind + React Query)",
  "type": "module",
  "engines": {
    "node": ">=18"
  },
  "scripts": {
    "dev": "next dev -p 3000",
    "build": "next build",
    "start": "next start -p 3000",
    "lint": "next lint",
    "format": "prettier --write \"**/*.{ts,tsx,js,jsx,json,md,css}\"",
    "test": "vitest",
    "test:unit": "vitest run",
    "test:e2e": "playwright test",
    "storybook": "start-storybook -p 6006 -s public",
    "build-storybook": "build-storybook -s public",
    "analyze": "cross-env ANALYZE=true next build"
  },
  "dependencies": {
    "next": "13.4.7",
    "react": "18.2.0",
    "react-dom": "18.2.0",
    "axios": "^1.5.0",
    "@tanstack/react-query": "^4.35.0",
    "clsx": "^1.2.1"
  },
  "devDependencies": {
    "typescript": "^5.3.0",
    "tailwindcss": "^3.5.0",
    "postcss": "^8.4.27",
    "autoprefixer": "^10.4.14",
    "eslint": "^8.40.0",
    "eslint-config-next": "13.4.7",
    "prettier": "^2.8.8",
    "vitest": "^1.3.8",
    "playwright": "^1.41.0",
    "@storybook/react": "^7.0.22",
    "start-storybook": "^7.0.22",
    "build-storybook": "^7.0.22",
    "cross-env": "^7.0.3"
  }
}

