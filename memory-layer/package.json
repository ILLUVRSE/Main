{
  "name": "memory-layer",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "build": "tsc -p tsconfig.json",
    "start": "node dist/service/server.js",
    "dev": "ts-node service/server.ts",
    "migrate": "npx ts-node scripts/runMigrations.ts memory-layer/sql/migrations",
    "vector-worker": "npx ts-node service/worker/vectorWorker.ts",
    "ttl-cleaner": "npx ts-node service/jobs/ttlCleaner.ts",
    "audit-verify": "npx ts-node service/audit/verifyTool.ts",
    "audit-replay": "npx ts-node tools/auditReplay.ts",
    "test:integration": "jest test/integration --runInBand",
    "lint": "eslint . --ext .ts"
  },
  "dependencies": {
    "@aws-sdk/client-kms": "^3.331.0",
    "@aws-sdk/client-s3": "^3.331.0",
    "@aws-sdk/lib-storage": "^3.331.0",
    "body-parser": "^1.20.2",
    "dotenv": "^17.2.3",
    "express": "^4.18.2",
    "node-fetch": "^3.3.2",
    "pg": "^8.16.3",
    "uuid": "^13.0.0"
  },
  "devDependencies": {
    "@types/express": "^4.17.20",
    "@types/jest": "^30.0.0",
    "@types/node": "^20.4.2",
    "@types/pg": "^8.11.10",
    "@types/supertest": "^2.0.12",
    "jest": "^30.2.0",
    "supertest": "^6.3.3",
    "ts-jest": "^29.1.1",
    "ts-node": "^10.9.1",
    "typescript": "^5.9.3",
    "eslint": "^8.46.0",
    "@typescript-eslint/eslint-plugin": "^6.21.0",
    "@typescript-eslint/parser": "^6.21.0"
  }
}

