{
  "name": "artifact-publisher-server",
  "version": "1.0.0",
  "private": true,
  "type": "module",
  "scripts": {
    "dev": "tsx src/index.ts",
    "build": "tsc -p tsconfig.json",
    "start": "node dist/index.js",
    "test": "vitest run",
    "test:watch": "vitest",
    "test:e2e": "vitest run test/e2e",
    "test:signed-proof": "vitest run test/e2e/signedProofs.e2e.test.ts",
    "test:unit": "vitest run test/unit",
    "migrate": "node scripts/runMigrations.js",
    "lint": "tsc --noEmit"
  },
  "dependencies": {
    "cors": "^2.8.5",
    "dotenv": "^16.4.5",
    "express": "^4.19.2",
    "pg": "^8.12.0",
    "seedrandom": "^3.0.5"
  },
  "devDependencies": {
    "@types/cors": "^2.8.19",
    "@types/express": "^4.17.21",
    "@types/node": "^20.12.12",
    "@types/seedrandom": "^3.0.8",
    "tsx": "^4.15.1",
    "typescript": "^5.6.3",
    "vitest": "^1.6.0"
  }
}
