{
  "Version": "2012-10-17",
  "Statement": [
    {
      "Sid": "AllowKmsAsymmetricSigning",
      "Effect": "Allow",
      "Action": [
        "kms:Sign",
        "kms:GetPublicKey",
        "kms:DescribeKey",
        "kms:GenerateMac"
      ],
      "Resource": "arn:aws:kms:REGION:ACCOUNT_ID:key/KEY_ID"
    }
  ]
}
